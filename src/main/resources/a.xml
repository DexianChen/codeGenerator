<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="">
	<select id="" parameterType="" resultMap="" >
		SELECT
		id, name
		FROM
		tb_user
		<where>
			<if test="id != null">
				id = #{id}
			</if>
			<if test="name != null">
				name = #{name}
			</if>
		</where>
	</select>
</mapper>